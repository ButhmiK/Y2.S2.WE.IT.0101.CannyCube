<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FunctionNavigate"
    tools:layout_editor_absoluteX="0dp"
    tools:layout_editor_absoluteY="46dp">


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="420dp"
        android:layout_height="140dp"
        android:layout_marginTop="4dp"
        android:adjustViewBounds="false"
        android:backgroundTint="#614545"
        android:cropToPadding="false"

        android:scaleType="centerCrop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bg" />


    <Button
        android:id="@+id/button8"
        android:layout_width="150dp"
        android:layout_height="68dp"
        android:layout_marginTop="92dp"
        android:backgroundTint="#ED888287"
        android:text="Income"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="747dp"
        android:layout_height="600dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/picture1" />

    <Button
        android:id="@+id/button10"
        android:layout_width="150dp"
        android:layout_height="68dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#DD6EBB"
        android:text="Company"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button5"
        app:layout_constraintVertical_bias="0.071" />

    <Button
        android:id="@+id/button5"
        android:layout_width="150dp"
        android:layout_height="68dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#74888287"
        android:text="Employee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="68dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="#F386D1"
        android:text="Expences"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

</android.support.constraint.ConstraintLayout>